<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.groceryshopjayshah">

    <application
        android:name=".PaymentInfo"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".PaymentScreen"></activity>
        <activity android:name=".GroceryBill" />
        <activity android:name=".GroceryOrder" />
        <activity android:name=".AdminUpdateGroceryScreen" />
        <activity android:name=".AdminDeleteGroceryScreen" />
        <activity android:name=".AdminAddGroceryScreen" />
        <activity android:name=".UserScreen" />
        <activity android:name=".AdminScreen" />
        <activity android:name=".Registration" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>